<?xml version="1.0"?>

<!-- Eurocopter EC 130 Helicopter by Heiko Schulz, animation set -->

<PropertyList>


	<path>bk117.ac</path>
	<offsets>
		<x-m>2.5</x-m>
		<z-m>-0.3</z-m>
		<pitch-deg>0.2</pitch-deg>
	</offsets>
	
<!--shader-->
<effect>
		<inherits-from>Aircraft/bk117/Models/Effects/bk117reflect</inherits-from>
		<object-name>fuselage</object-name>
		<object-name>frontdoor_right</object-name>
		<object-name>frontdoor_left</object-name>
		<object-name>backdoor_right</object-name>
		<object-name>backdoor_left</object-name>
		<object-name>radom</object-name>
		<object-name>wire_cutter</object-name>
		<object-name>skid</object-name>
		<object-name>skid_floats</object-name>
		<object-name>sandfilter</object-name>
		<object-name>reardoor_right</object-name>
		<object-name>reardoor_left</object-name>
		<object-name>reardoor_wind_right</object-name>
		<object-name>reardoor_wind_left</object-name>
		<object-name>radom</object-name>
		<object-name>nose</object-name>
		<object-name>Plane.009</object-name>
		<object-name>Plane.010</object-name>
		<object-name>nacaintake</object-name>
</effect>

<effect>
		<inherits-from>Aircraft/bk117/Models/Effects/bk117reflectglas</inherits-from>
		<object-name>frontdoorglas_left</object-name>
		<object-name>frontdoorglas_right</object-name>
		<object-name>window</object-name>
		<object-name>backdoorglas_right</object-name>
		<object-name>backdoorglas_left</object-name>
		<object-name>rearwind_left</object-name>
		<object-name>rearwind_right</object-name>	
</effect>
	
<!--animation-->

<params>
		<flapA>
			<factor>0.3</factor>
		</flapA>
		<flapB>
			<factor>0.25</factor>
		</flapB>
		<flapC>
			<factor>0.2</factor>
		</flapC>
		<flapD>
			<factor>0.2</factor>
		</flapD>
		<flapE>
			<factor>0.15</factor>
		</flapE>
	</params>



